{
  "name": "appointment_scheduler_webapp_expressmongodb",
  "version": "1.3.0",
  "private": true,
  "description": "appointment scheduler web server with express and mongodbatlas",
  "main": "index.js",
  "scripts": {
    "server": "nodemon --exec \"npm run lint && node index\"",
    "client": "npm run serve --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "render": "npm install --omit=dev && npm install ---force --legacy-peer-deps -omit=dev --prefix client && npm run build --prefix client",
    "start": "node index.js",
    "lint": "eslint .  --ext .js"
  },
  "author": "Leindfraust",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chatgpt": "^5.2.5",
    "cloudinary": "^1.37.1",
    "connect-history-api-fallback": "^2.0.0",
    "connect-mongo": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "formidable": "^2.1.1",
    "helmet": "^7.0.0",
    "mongoose": "^7.3.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.3",
    "puppeteer": "^20.7.1",
    "socket.io": "^4.6.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-plugin-cypress": "^2.13.3",
    "nodemon": "^2.0.22"
  }
}